{
  "env": { "browser": true, "es2021": true, "node": true },
  "overrides": [
    {
      "extends": [],
      "files": ["**/*.?(ts|js)?(x)"],
      "parser": "@babel/eslint-parser",
      "parserOptions": {
        "requireConfigFile": false,
        "sourceType": "module",
        "ecmaFeatures": { "jsx": true },
        "babelOptions": { "presets": ["react-app"] }
      },
      "plugins": ["jsx-a11y", "react-hooks", "react", "@next/next"],
      "settings": { "react": { "version": "detect" } },
      "overrides": [],
      "rules": {
        "react/boolean-prop-naming": 0,
        "react/button-has-type": 2,
        "react/default-props-match-prop-types": 0,
        "react/destructuring-assignment": 0,
        "react/display-name": 0,
        "react/forbid-dom-props": 0,
        "react/forbid-elements": 0,
        "react/forbid-foreign-prop-types": [1, { "allowInPropTypes": true }],
        "react/forbid-prop-types": 0,
        "react/function-component-definition": [
          1,
          {
            "namedComponents": "function-declaration",
            "unnamedComponents": "arrow-function"
          }
        ],
        "react/jsx-boolean-value": 2,
        "react/jsx-child-element-spacing": 0,
        "react/jsx-closing-bracket-location": 0,
        "react/jsx-closing-tag-location": 0,
        "react/jsx-curly-brace-presence": [
          1,
          { "children": "never", "props": "never" }
        ],
        "react/jsx-curly-newline": 0,
        "react/jsx-curly-spacing": 0,
        "react/jsx-equals-spacing": 0,
        "react/jsx-filename-extension": 0,
        "react/jsx-first-prop-new-line": 0,
        "react/jsx-fragments": [1, "syntax"],
        "react/jsx-handler-names": [
          1,
          {
            "checkLocalVariables": false,
            "eventHandlerPrefix": "handle",
            "eventHandlerPropPrefix": "on"
          }
        ],
        "react/jsx-indent": 0,
        "react/jsx-indent-props": 0,
        "react/jsx-key": [
          1,
          { "checkFragmentShorthand": true, "checkKeyMustBeforeSpread": true }
        ],
        "react/jsx-max-depth": 0,
        "react/jsx-max-props-per-line": 0,
        "react/jsx-newline": 0,
        "react/jsx-no-bind": 0,
        "react/jsx-no-comment-textnodes": 1,
        "react/jsx-no-constructed-context-values": 1,
        "react/jsx-no-duplicate-props": 2,
        "react/jsx-no-literals": 0,
        "react/jsx-no-script-url": 1,
        "react/jsx-no-target-blank": 1,
        "react/jsx-no-undef": 0,
        "react/jsx-no-useless-fragment": 1,
        "react/jsx-one-expression-per-line": 0,
        "react/jsx-pascal-case": [1, { "allowAllCaps": true, "ignore": [] }],
        "react/jsx-props-no-multi-spaces": 0,
        "react/jsx-props-no-spreading": 0,
        "react/jsx-sort-default-props": 0,
        "react/jsx-sort-props": 0,
        "react/jsx-space-before-closing": 0,
        "react/jsx-tag-spacing": 0,
        "react/jsx-uses-react": 1,
        "react/jsx-uses-vars": 0,
        "react/jsx-wrap-multilines": 0,
        "react/no-access-state-in-setstate": 0,
        "react/no-adjacent-inline-elements": 0,
        "react/no-array-index-key": 1,
        "react/no-arrow-function-lifecycle": 0,
        "react/no-children-prop": 2,
        "react/no-danger": 1,
        "react/no-danger-with-children": 2,
        "react/no-deprecated": 2,
        "react/no-did-mount-set-state": 0,
        "react/no-did-update-set-state": 1,
        "react/no-direct-mutation-state": 2,
        "react/no-find-dom-node": 2,
        "react/no-invalid-html-attribute": 0,
        "react/no-is-mounted": 2,
        "react/no-multi-comp": 0,
        "react/no-redundant-should-component-update": 2,
        "react/no-render-return-value": 0,
        "react/no-set-state": 0,
        "react/no-string-refs": 2,
        "react/no-this-in-sfc": 2,
        "react/no-typos": 2,
        "react/no-unescaped-entities": 0,
        "react/no-unknown-property": 1,
        "react/no-unsafe": 2,
        "react/no-unused-prop-types": 1,
        "react/no-unused-state": 1,
        "react/no-will-update-set-state": 1,
        "react/prefer-es6-class": 2,
        "react/prefer-read-only-props": 0,
        "react/prefer-stateless-function": 0,
        "react/prop-types": 0,
        "react/react-in-jsx-scope": 0,
        "react/require-default-props": 0,
        "react/require-optimization": 0,
        "react/require-render-return": 2,
        "react/self-closing-comp": 1,
        "react/sort-comp": 0,
        "react/sort-prop-types": 0,
        "react/state-in-constructor": 0,
        "react/static-property-placement": [2, "static public field"],
        "react/style-prop-object": 1,
        "react/void-dom-elements-no-children": 2,
        "jsx-a11y/accessible-emoji": 0,
        "jsx-a11y/alt-text": 2,
        "jsx-a11y/anchor-has-content": 2,
        "jsx-a11y/anchor-is-valid": [
          2,
          {
            "aspects": ["invalidHref", "noHref", "preferButton"],
            "components": []
          }
        ],
        "jsx-a11y/aria-activedescendant-has-tabindex": 1,
        "jsx-a11y/aria-props": 2,
        "jsx-a11y/aria-proptypes": 2,
        "jsx-a11y/aria-role": [1, { "ignoreNonDOM": true }],
        "jsx-a11y/aria-unsupported-elements": 2,
        "jsx-a11y/click-events-have-key-events": 1,
        "jsx-a11y/control-has-associated-label": 1,
        "jsx-a11y/heading-has-content": 1,
        "jsx-a11y/html-has-lang": 1,
        "jsx-a11y/iframe-has-title": 1,
        "jsx-a11y/img-redundant-alt": 1,
        "jsx-a11y/interactive-supports-focus": 2,
        "jsx-a11y/label-has-associated-control": 2,
        "jsx-a11y/label-has-for": 0,
        "jsx-a11y/lang": 2,
        "jsx-a11y/media-has-caption": 1,
        "jsx-a11y/mouse-events-have-key-events": 2,
        "jsx-a11y/no-access-key": 1,
        "jsx-a11y/no-autofocus": 2,
        "jsx-a11y/no-distracting-elements": 1,
        "jsx-a11y/no-interactive-element-to-noninteractive-role": 2,
        "jsx-a11y/no-noninteractive-element-interactions": 2,
        "jsx-a11y/no-noninteractive-element-to-interactive-role": [
          2,
          {
            "li": ["menuitem", "option", "row", "tab", "treeitem"],
            "ol": [
              "listbox",
              "menu",
              "menubar",
              "radiogroup",
              "tablist",
              "tree",
              "treegrid"
            ],
            "table": ["grid"],
            "td": ["gridcell"],
            "ul": [
              "listbox",
              "menu",
              "menubar",
              "radiogroup",
              "tablist",
              "tree",
              "treegrid"
            ]
          }
        ],
        "jsx-a11y/no-noninteractive-tabindex": 2,
        "jsx-a11y/no-onchange": 0,
        "jsx-a11y/no-redundant-roles": 1,
        "jsx-a11y/no-static-element-interactions": 2,
        "jsx-a11y/role-has-required-aria-props": 2,
        "jsx-a11y/role-supports-aria-props": 2,
        "jsx-a11y/scope": 2,
        "jsx-a11y/tabindex-no-positive": 2,
        "react-hooks/exhaustive-deps": 2,
        "react-hooks/rules-of-hooks": 2
      }
    },
    {
      "extends": [],
      "files": ["**/*.ts?(x)"],
      "parser": "@typescript-eslint/parser",
      "parserOptions": {
        "ecmaFeatures": { "jsx": true },
        "project": "./tsconfig.json",
        "warnOnUnsupportedTypeScriptVersion": false
      },
      "plugins": ["@typescript-eslint"],
      "settings": { "react": { "version": "detect" } },
      "rules": {
        "@typescript-eslint/adjacent-overload-signatures": 2,
