    "no-implicit-globals": 2,
    "no-implied-eval": 2,
    "no-invalid-this": 0,
    "no-iterator": 0,
    "no-labels": 2,
    "no-lone-blocks": 0,
    "no-loop-func": 0,
    "no-magic-numbers": 0,
    "no-multi-spaces": 0,
