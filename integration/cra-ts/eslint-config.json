        "jest/no-hooks": 0,
        "jest/no-identical-title": 2,
        "jest/no-if": 2,
        "jest/no-interpolation-in-snapshots": 2,
        "jest/no-jasmine-globals": 2,
        "jest/no-jest-import": 2,
        "jest/no-large-snapshots": [1, { "maxSize": 300 }],
        "jest/no-mocks-import": 2,
        "jest/no-restricted-matchers": 0,
        "jest/no-standalone-expect": 0,
        "jest/no-test-prefixes": 2,
        "jest/no-test-return-statement": 2,
        "jest/no-truthy-falsy": 0,
        "jest/no-try-expect": 0,
        "jest/prefer-called-with": 2,
        "jest/prefer-expect-assertions": 0,
        "jest/prefer-hooks-on-top": 2,
        "jest/prefer-inline-snapshots": 0,
        "jest/prefer-spy-on": 1,
        "jest/prefer-strict-equal": 1,
        "jest/prefer-to-contain": 1,
        "jest/prefer-to-have-length": 1,
        "jest/prefer-todo": 1,
        "jest/require-to-throw-message": 0,
        "jest/require-top-level-describe": 2,
        "jest/valid-expect": 2,
        "jest/valid-expect-in-promise": 2,
        "jest/valid-title": 1,
        "jest-dom/prefer-checked": 1,
        "jest-dom/prefer-empty": 1,
        "jest-dom/prefer-enabled-disabled": 1,
        "jest-dom/prefer-focus": 1,
        "jest-dom/prefer-in-document": 1,
        "jest-dom/prefer-required": 1,
        "jest-dom/prefer-to-have-attribute": 1,
        "jest-dom/prefer-to-have-class": 1,
        "jest-dom/prefer-to-have-style": 1,
        "jest-dom/prefer-to-have-text-content": 1,
        "jest-dom/prefer-to-have-value": 1,
        "testing-library/await-async-query": 2,
        "testing-library/await-async-utils": 2,
        "testing-library/await-fire-event": 0,
        "testing-library/consistent-data-testid": 0,
        "testing-library/no-await-sync-events": 2,
        "testing-library/no-await-sync-query": 2,
        "testing-library/no-container": 1,
        "testing-library/no-debugging-utils": 2,
        "testing-library/no-dom-import": [1, "react"],
        "testing-library/no-node-access": 1,
        "testing-library/no-manual-cleanup": 2,
        "testing-library/no-promise-in-fire-event": 2,
        "testing-library/no-render-in-setup": 0,
        "testing-library/no-unnecessary-act": [2, { "isStrict": true }],
        "testing-library/no-wait-for-empty-callback": 2,
