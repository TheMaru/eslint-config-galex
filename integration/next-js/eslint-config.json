      },
      "plugins": [
        "jsx-a11y",
        "react-hooks",
        "react",
        "@next/next"
      ],
      "settings": {
        "react": {
          "version": "detect"
        }
      },
      "overrides": [
        {
          "files": [
            "src/pages/**/*.?(t|j)s?(x)",
