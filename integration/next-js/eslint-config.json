{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "overrides": [
    {
      "extends": [],
      "files": [
        "**/*.?(ts|js)?(x)"
      ],
      "parser": "@babel/eslint-parser",
      "parserOptions": {
        "requireConfigFile": false,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
